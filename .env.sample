# OHDSI WebAPI Client Configuration
# Copy this file to .env and customize for your environment

# WebAPI Server Configuration
OHDSI_WEBAPI_BASE_URL=https://atlas-demo.ohdsi.org/WebAPI

# Caching Configuration
OHDSI_CACHE_ENABLED=true
OHDSI_CACHE_TTL=300
OHDSI_CACHE_MAX_SIZE=128

# Integration Testing (for live tests)
INTEGRATION_WEBAPI=0

# Authentication (if needed)
# WEBAPI_USERNAME=your_username
# WEBAPI_PASSWORD=your_password
# WEBAPI_TOKEN=your_bearer_token

# HTTP Configuration
# WEBAPI_TIMEOUT=30.0
# WEBAPI_VERIFY_SSL=true

# Development/Debug
# OHDSI_DEBUG=false
# OHDSI_LOG_LEVEL=INFO
